{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Debug main.go",
      "type": "go",
      "request": "launch",
      "mode": "debug",
      "program": "${file}"
    },
    {
      "name": "Debug Package",
      "type": "go",
      "request": "launch",
      "mode": "debug",
      "program": "${workspaceFolder}"
    },
    {
      "name": "Attach to Process",
      "type": "go",
      "request": "attach",
      "processId": "${command:pickProcess}"
    }
  ]
}
